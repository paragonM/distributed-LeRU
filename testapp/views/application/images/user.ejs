        <h1 class="fw-light">User dashboard</h1>
        <p class="lead text-muted">Welcome to LeRU, below are all stores registered to your user account</p>
        <p>
          <a href="/addstore/<%= param %>" class="btn btn-primary my-2">Add a store</a>
          <a href="#" class="btn btn-secondary my-2">Store information</a>
        </p>
      </div>
    </div>
  </section>

  <div class="album py-5 bg-light">
    <div class="container">

      <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
<% try{ %>
<% if(array1.length == 0){%>
<h2 class="center">No stores registered</h2>
 <% } %>
 <% if(array1.length!=0) {%>
   <% for(let i = 0;i < array1.length;i++){ %>
        <div class="col">
          <div class="card shadow-sm">
            <svg class="bd-placeholder-img card-img-top" width="100%" height="225" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: Thumbnail" preserveAspectRatio="xMidYMid slice" focusable="false"><title>Placeholder</title><rect width="100%" height="100%" fill="#55595c"/><text x="50%" y="50%" fill="#eceeef" dy=".3em"><%= array1[i][3] %></text></svg>

            <div class="card-body">
              <h5 class="card-text"><%= array1[i][2] %></h5>
              <h5 class="card-text"><%= array1[i][1] %></h5>
              <div class="d-flex justify-content-between align-items-center">
                <div class="btn-group">
                  <a href="/inventory/<%=param%>/<%= array1[i][0] %>" type="button" class="btn btn-sm btn-outline-secondary">Inventory</a>
                  <a href="/pos/<%=array1[i][0]+"/"+param%>" type="button" class="btn btn-sm btn-outline-secondary">Open POS</a>
                  <a href="/<%= array1[i][0] %>/pos"type="button" class="btn btn-sm btn-outline-secondary">About</a>

                </div>

              </div>
            </div>
          </div>
        </div>
        <% }%>
<% } %>
<% }catch{ } %>
<%  %>
      </div>
    </div>
  </div>

</main>

